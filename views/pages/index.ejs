<!DOCTYPE html>

<html lang="en">

<head>
  <%- include('../partial/head') %>
  <title>Book App</title>
</head>

<body>
  <header>
    <%- include('../partial/nav') %>
  </header>

  <main>
    <ul>
      <% books.forEach ( book => { %>
        <li>
          <img src="<%= book.image_url %>">
          <%= book.title %>
          <%= book.author %>
          <%= book.isbn %>
          <%= book.description %>
          <%= book.bookshelf %>
          <%= book.listPrice %>
        </li>
      <% }) %>
    </ul>
  </main>

  <footer>&copy; 2020 Natalie Sinner and Bhagirath Bhatt</footer>
</body>

</html>